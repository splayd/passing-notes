{
  "name": "passing-notes",
  "version": "0.0.0",
  "description": "Workflows for transfering data between frontend and backend",
  "keywords": [],
  "homepage": "https://github.com/splayd/passing-notes",
  "bugs": "https://github.com/splayd/passing-notes/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "main": "./src/index.js",
  "bin": {
    "pass-notes": "./src/cli/pass-notes/bin.js"
  },
  "browser": {
    "./src/index.js": "./src/index.browser.js",
    "./lib/middleware/compress-body/index.js": "./lib/middleware/compress-body/index.browser.js",
    "./lib/middleware/resolve-absolute-url/index.js": "./lib/middleware/resolve-absolute-url/index.browser.js",
    "./lib/middleware/serialize-json/index.js": "./lib/middleware/serialize-json/index.browser.js",
    "./lib/middleware/serve-ui/index.js": "./lib/middleware/serve-ui/index.browser.js",
    "./lib/middleware/set-content-length/index.js": "./lib/middleware/set-content-length/index.browser.js",
    "./lib/rpc/lift-responder/index.js": "./lib/rpc/lift-responder/index.browser.js"
  },
  "repository": "splayd/passing-notes",
  "scripts": {
    "prepare": "flow-typed update --skip",
    "test": "standard -v --fix && flow && ava",
    "prepack": "build-esm"
  },
  "dependencies": {
    "babel-preset-diff": "^3.0.0",
    "babel-register": "^6.26.0",
    "compressible": "^2.0.13",
    "content-type": "^1.0.4",
    "cross-fetch": "^2.1.1",
    "fs-extra": "^6.0.0",
    "get-port": "^3.2.0",
    "is-absolute-url": "^2.1.0",
    "lodash": "^4.17.10",
    "negotiator": "^0.6.1",
    "parcel-bundler": "^1.8.1",
    "promisify-event": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "build-esm": "^3.2.0",
    "flow-bin": "0.69.0",
    "flow-typed": "^2.4.0",
    "puppet-strings": "^0.0.5",
    "standard-esnext": "^4.0.3",
    "tempy": "^0.2.1"
  },
  "babel": {
    "presets": [
      "diff"
    ]
  },
  "ava": {
    "concurrency": 8,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
